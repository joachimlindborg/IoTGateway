<?xml version='1.0' encoding='utf-8'?>
<SimpleXmppConfiguration xmlns='http://waher.se/Schema/SimpleXmppConfiguration.xsd'>
	<Host>ENTER HOST NAME HERE.</Host>
	<Port>5222</Port>
	<Account>ENTER ACCOUNT NAME HERE.</Account>
	<Password>ENTER PASSWORD HERE.</Password>
	<ThingRegistry>ENTER JID OF THING REGISTRY HERE, OR LEAVE BLANK.</ThingRegistry>
	<Provisioning>ENTER JID OF PROVISIONING SERVER HERE, OR LEAVE BLANK.</Provisioning>
	<Events>ENTER JID OF XMPP EVENT SINK, OR LEAVE BLANK.</Events>
	<Sniffer>true</Sniffer>
	<TrustServer>false</TrustServer>
	<AllowCramMD5>true</AllowCramMD5>
	<AllowDigestMD5>true</AllowDigestMD5>
	<AllowPlain>false</AllowPlain>
	<AllowScramSHA1>true</AllowScramSHA1>
	<AllowEncryption>true</AllowEncryption>
	<RequestRosterOnStartup>true</RequestRosterOnStartup>
  <AllowRegistration>false</AllowRegistration>
  <FormSignatureKey></FormSignatureKey>
  <FormSignatureSecret></FormSignatureSecret>
</SimpleXmppConfiguration>
